module diamond.errors.exceptions.domexception;

final class DomException : Exception
{
  public:
  /**
  * Creates a new dom exception.
  * Params:
  *   message =   The message.
  *   fn =        The file.
  *   ln =        The line.
  */
  this(string message, string fn = __FILE__, size_t ln = __LINE__) @safe
  {
    super(message, fn, ln);
  }
}
